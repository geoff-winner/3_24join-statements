<html>
    <head>
        <title>Courses List</title>
    </head>
    <body>
        <h1>List of courses:</h1>

        {% if courses is not empty %}

            {% for course in courses %}
                <p><a href='courses/{{ course.getId }}'>{{ course.getCourse }}</a></p>
            {% endfor %}

        {% endif %}
    </body>
</html>
